    LINKER        = alink
    LINK_FLAGS    = 
    RC_COMPILE    = arc
    RC_FLAGS      =
    INTERMEDIATE_DEBUG   = .debug
    INTERMEDIATE_RELEASE = .release
// $START-AUTODEPEND
    collat.ch
    socket.ch
    os.ch
    fileio.ch
    prompt.ch
    memvar.ch
    std.ch
