// $STOP-AUTODEPEND
    HTTPJOB.PRG
    HTTPSERVER.PRG
    HTTPSERVICE.PRG
    LOGGER.PRG

[HTTPCTR.EXE]
    COMPILE       = xpp
    COMPILE_FLAGS = /q
    DEBUG_SAVE    = yes
    GUI           = no
    LINKER        = alink
    LINK_FLAGS    = 
