[PROJECT]
    DEBUG         = yes
    VERSION       = 2.3
    PROJECT.XPJ

[PROJECT.XPJ]
    HTTPSERVICE.EXE
    HTTPCTR.EXE

[HTTPSERVICE.EXE]
